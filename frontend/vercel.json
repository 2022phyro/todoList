{
    "version": 2,
    "name": "todo-list",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        {
            "src": "/(.*).(js|css|json|png|jpg|svg)",
            "dest": "/dist/$1.$2",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}